{
  "$ref": "#/definitions/AddressI",
  "definitions": {
    "UserI": {
      "type": "object",
      "properties": {
        "id": {
          "type": "number"
        },
        "name": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "email": {
          "pattern": "^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$",
          "type": "string"
        },
        "profileURL": {
          "type": "string"
        },
        "role": {
          "enum": [
            "ADMIN",
            "CLIENT"
          ],
          "type": "string"
        },
        "birthDate": {
          "type": "string",
          "format": "date-time"
        },
        "address": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AddressI"
          }
        }
      },
      "required": [
        "birthDate",
        "email",
        "name",
        "password",
        "role",
        "username"
      ]
    },
    "AddressI": {
      "type": "object",
      "properties": {
        "id": {
          "type": "number"
        },
        "state": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "zipCode": {
          "minLength": 5,
          "type": "string"
        },
        "street": {
          "type": "string"
        },
        "neighborhood": {
          "type": "string"
        },
        "additionalData": {
          "type": "string"
        },
        "interiorCode": {
          "type": "string"
        },
        "exteriorCode": {
          "type": "string"
        },
        "userId": {
          "type": "number"
        },
        "user": {
          "$ref": "#/definitions/UserI"
        }
      },
      "required": [
        "city",
        "id",
        "interiorCode",
        "neighborhood",
        "state",
        "street",
        "zipCode"
      ]
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}